<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>
    <?xml-stylesheet type="text/css" href="#style"?>
    <extra id="style">
        .grid {
          display: grid;
        }
        .fill {
          grid-template-columns: repeat(auto-fill, minmax(100px, 1fr));
        }
        .fit {
          grid-template-columns: repeat(auto-fit, minmax(100px, 1fr));
        }
    </extra>
    <category label="Category uno">
        <frame class="grid fill" side="top" fill="x">
            <label name="a" text="A" bg="gray"/>
            <label name="b" text="B"  bg="blue"/>
            <label name="c" text="C"  bg="yellow"/>
            <label name="d" text="D"  bg="green"/>
            <label name="e" text="E" bg="red"/>
            <label text="col1" bg="gray"/>
            <label text="col2" bg="gray"/>
        </frame>
        <frame class="grid fit" side="bottom" fill="x">
            <label name="a" text="A" bg="gray"/>
            <label name="b" text="B"  bg="blue"/>
            <label name="c" text="C"  bg="yellow"/>
            <label name="d" text="D"  bg="green"/>
            <label name="e" text="E" bg="red"/>
            <label text="col1" bg="gray"/>
            <label text="col2" bg="gray"/>
        </frame>
    </category>
</settings>

