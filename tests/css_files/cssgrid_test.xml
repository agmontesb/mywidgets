<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>
    <?xml-stylesheet type="text/css" href="#style"?>
    <extra id="style">
        #grid {
          display: grid;
          grid-template-columns: repeat(8, 1fr);
          grid-template-rows: repeat(8, 1fr);
          place-items: stretch;
        }
        #a { grid-column: 1 / span 8; grid-row: -4 / span 3; }
        #b { grid-column: 1 / span 4; grid-row: 1 / span 4; z-index: 10; }
        #c { grid-column: 4 / span 5; grid-row: 1 / span 3; }
        #d { grid-column: -4 / span 3; grid-row: -5 / span 3; }
        #e { grid-column: 3 / span 4; grid-row: 3 / span 4;
             z-index: 5; }
    </extra>
    <category label="Category uno">
        <frame name="grid" side="top" fill="both" expand="yes">
            <label name="a" text="A" bg="gray" height="0"/>
            <label name="b" text="B"  bg="blue"/>
            <label name="c" text="C"  bg="yellow"/>
            <label name="d" text="D"  bg="green"/>
            <label name="e" text="E" bg="red"/>
<!--            <label text="col1" bg="gray"/>-->
