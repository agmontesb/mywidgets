<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>
    <?xml-stylesheet type="text/css" href="#style"?>
    <extra id="style">
        .grid {
            display: grid;
            grid-template-columns: repeat(3, 50px);
            grid-template-rows: repeat(3, 50px);
            grid-gap: 0px 0px;
            grid-auto-columns: 50px;
            grid-auto-rows: 50px;
            justify-items:stretch;
            align-items:stretch;
        }
        #uno {
        	grid-column-start: 3;
            grid-row-start: 2;
        }
        .row {
          grid-column-start: 1;
          grid-column-end: -1;
        }
        .column {
          grid-row-start: 1;
          grid-row-end: 3;
          bg:light green;
          bd:4;
        }
        .blk {
            height:10;
            width:10;
            bg:black;
        }

    </extra>
    <category label="Category uno">
        <frame class="grid" side="top" fill="both" expand="yes">
            <label name="uno" text="p1" bg="gray"/>
            <label text="row2"  bg="blue"/>
            <label text="row3"  bg="yellow"/>
            <label text="row4"  bg="green"/>
            <label text="row1" bg="red"/>
            <label text="col1" bg="gray"/>
            <label text="col2" bg="gray"/>
            <label text="col3" bg="gray"/>
            <label text="col4" bg="gray"/>
<!--            <label text="col1" bg="gray"/>-->
<!--            <label text="col2" bg="gray"/>-->
<!--            <label text="col3" bg="gray"/>-->
<!--            <label text="col4" bg="gray"/>-->
<!--            <label text="col1" bg="gray"/>-->
<!--            <label text="col2" bg="gray"/>-->
<!--            <label text="col3" bg="gray"/>-->
<!--            <label text="col4" bg="gray"/>-->
        </frame>
    </category>
</settings>

